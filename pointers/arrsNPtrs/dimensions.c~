#include <stdio.h>

void display2DArrayUnknownSize(int *arr, int rows, int cols);

int main(void)
{
    
}

void display2DArrayUnknownSize(int *arr, int rows, int cols) {
    for(int i=0; i<rows; i++) {
        for(int j=0; j<cols; j++) {
            printf("%d ", *(arr + (i*cols) + j));
        }
        printf("\n");
    }
}
